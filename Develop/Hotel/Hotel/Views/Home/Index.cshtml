
@{
    ViewBag.Title = "Index";
}
<!DocType html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Hotel the Happiness</title>
    <link rel="stylesheet" type="text/css" href="~/Content/bootstrap.css" />
    <script src="~/Scripts/jquery-1.9.1.min.js"></script>
    <script src="~/Scripts/bootstrap.js"></script>
    <script src="~/Scripts/angular.js"></script>
    <script src="~/Scripts/Hotel.js"></script>
    
</head>
<body>
    <h2>Hotel the Happiness</h2>
    <img src="data:image/jpeg;base64,"/ >
    <p></p>
    <a style="position: fixed; bottom: 0; text-align: center; right: 20px" href="http://www.google.com" target="_blank"> Google </a>  

    <script>
         
        var Rooms = [];
         @foreach (var Room in ViewBag.rooms)
        {
            @:Rooms.push(@Html.Raw(Json.Encode(Room)));
                    
        }
    </script>
    <div class="container" ng-app="Hotel" ng-controller="HotelController">
        <h2>Rooms</h2>
         <table class="table table-bordered">
            <thead>
                <tr>
                    <th>Room number</th>
                    <th>Capacity</th>
                    <th>Price</th>
                 </tr>
            </thead>
            <tbody>
                @{
                    for (var i = 0; i < ViewBag.rooms.Count; ++i)
                    {
                        var room = ViewBag.rooms[i];

                        <tr id="room-@i">
                            <td>@room.RoomNumber</td>
                            <td>@room.Capacity</td>
                            <td>&euro; @room.Price</td>
                        </tr>

                    }
                    @*@ViewBag.rooms[0].RoomNumber
                        @ViewBag.rooms[0].Capacity
                        @ViewBag.rooms[0].Price*@
                }
            </tbody>

        </table>
        <form class="form-inline">
            <div class="form-group">
                <label for="name">Name</label>
                <input type="text" id="name" class="form-control" />
            </div>
            <div class="form-group">
                <label for="count">Total guests</label>
                <input type="number" id="count" class="form-control" />
            </div>

            <div class="form-group">
                <label for="date">Check in date</label>
                <input type="date" id="date" class="form-control" />
            </div>
            <div class="form-group">
                <label for="nights">Total night(s)</label>
                <input type="number" id="nights" class="form-control" />
            </div>
            <button type="button" id="reserve" class="btn btn-default">Reserveer</button>
        </form>
        <h2>The reservation</h2>
        <table class="table table-condensed table-striped">
            <thead>
                <tr>
                    <th>total guests</th>
                    <th>Name</th>
                    <th>Check in date</th>
                    <th>Check out date</th>
                </tr>
            </thead>
            
        </table>
        </div>

</body>

</html>


